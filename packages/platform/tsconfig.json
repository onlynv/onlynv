{
	"extends": "@onlynv/tsconfig/base.json",
	"compilerOptions": {
		"lib": ["dom", "dom.iterable", "esnext"],
		"moduleResolution": "bundler",
		"jsx": "preserve",
		"incremental": true,
		"plugins": [
			{
				"name": "next"
			}
		],
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
	"exclude": ["node_modules"]
}
