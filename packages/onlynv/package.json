{
	"name": "onlynv",
	"description": "A smaller, faster, replacement for dotenv.",
	"version": "0.2.2",
	"author": "TheCommieAxolotl",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/onlynv/onlynv",
		"type": "git",
		"directory": "packages/onlynv"
	},
	"bugs": {
		"url": "https://github.com/onlynv/onlynv/issues"
	},
	"homepage": "https://github.com/onlynv/onlynv/tree/main/packages/onlynv#readme",
	"scripts": {
		"build": "vite build",
		"dev": "vite build --watch",
		"test": "vitest",
		"bench": "vitest bench",
		"check-exports": "attw --pack .",
		"prepublishOnly": "vite build"
	},
	"types": "./index.d.ts",
	"files": [
		"dist",
		"index.d.ts"
	],
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./index.d.ts"
		},
		"./config": {
			"import": "./dist/config.js",
			"require": "./dist/config.cjs",
			"types": "./index.d.ts"
		}
	},
	"devDependencies": {
		"@onlynv/shared": "workspace:*",
		"@onlynv/tsconfig": "workspace:*",
		"@types/node": "^22.14.1",
		"dotenv": "^16.5.0",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"vite-plugin-dts": "^4.5.3",
		"vitest": "^3.1.1"
	}
}
